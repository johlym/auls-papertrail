<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>com.johnathanlyman.axe.plist</string>
  <key>ProgramArguments</key>
  <array>
  	<string>/usr/bin/ruby</string>
  	<string>/etc/papertrail/axe/axe.rb</string>
  </array>
  <key>RunAtLoad</key>
  <true/>
  <key>KeepAlive</key>
  <dict>
  	<key>NetworkState</key>
  	<true/>
    <key>Crashed</key>
	  <true/>
  </dict>
  <key>Nice</key>
  <integer>-5</integer>
</dict>
</plist>
